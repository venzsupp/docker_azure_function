{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
        "http": {
          "dynamicThrottlesEnabled": false,
          "maxOutstandingRequests": 200,
          "maxConcurrentRequests": 50
        }
      }
}